{
  "short_name": "React App",
  "name": "React Extension",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "React Ext",
    "default_icon": "icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "background",
    "storage",
    "history"
  ],
  "version": "1.0"
}