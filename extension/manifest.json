{
  "short_name": "hasBrain tracker",
  "name": "hasBrain tracker",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "public/img/icon.png"
  },
  "background": {
    "scripts": [
      "injected/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "background",
    "storage",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "public/js/axios.js",
        "public/js/jquery.js",
        "public/js/keywords.js",
        "injected/highlight.js",
        "public/js/semantic.min.js"
      ],
      "css": [
        "public/css/mycss.css"
      ]
    }
  ],
  "version": "3.8",
  "web_accessible_resources": [
    "/pages/popup.html",
    "/pages/options.html",
    "public/*",
    "/pages/js/jquery.min.js"
  ],
  "externally_connectable": {
    "matches": ["*://*.hasbrain.surge.sh/*"]
  },
  "options_page": "pages/options.html"
}