{
  "short_name": "hasBrain tracker",
  "name": "hasBrain tracker",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "public/img/icon.png"
  },
  "background": {
    "scripts": [
      "injected/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "background",
    "storage",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "public/js/axios.js",
        "public/js/jquery.js",
        "public/js/keywords.js",
        "injected/highlight.js",
        "public/js/semantic.min.js",
        "public/js/jwt-decode.js"
      ],
      "css": [
        "public/css/mycss.css"
      ]
    }
  ],
  "version": "3.20",
  "web_accessible_resources": [
    "/pages/popup.html",
    "/pages/refresh_token.html",
    "/pages/loading.html",
    "/pages/getting_tags.html",
    "/pages/options.html",
    "/pages/newtab",
    "/pages/newtab/",
    "/pages/newtab/index.html",
    "public/*",
    "/pages/js/jquery.min.js",
    "/pages/newtab/*.js"
  ],
  "externally_connectable": {
    "matches": ["*://*.hasbrain.surge.sh/*"]
  },
  "chrome_url_overrides" : { 
    "newtab": "/pages/newtab/index.html" 
  },
  "options_page": "pages/options.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}